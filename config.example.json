{
  "limited_user": "user",
  "dig_opts": {
    "retries": 10,
    "timeout": 3
  },
  "global": {
    "whitelisted": {
      "egress": {
	      "ipset_name": "global-container-egress-wl",
        "host_addr": [
          { "ip": "1.1.1.1", "dports": ["tcp:853"] },
          { "ip": "8.8.8.8", "dports": ["tcp:853"] }
        ]
      }
    }
  },
  "hosts": [
    {
      "name": "EXAMPLE",
      "src": "10.137.0.11",
      "whitelisted": {
	    "egress": {
          "ipset_name": "example-egress-wl",
      	  "domain_names": [
            { "name": "example.com", "dports": ["tcp:443"] },
            { "name": "example.net", "dports": ["tcp:443"] }
	        ]
	      }
      }
    },
    {
      "name": "EXAMPLE2",
      "src": "10.137.0.22",
      "whitelisted": {
	    "egress": {
          "ipset_name": "example2-egress-wl",
      	  "domain_names": [
            { "name": "example.com", "dports": ["tcp:443"] },
            { "name": "example.net", "dports": ["tcp:443"] }
          ]
        }
      }
    }
  ]
}
